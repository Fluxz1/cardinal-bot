{
	"name": "cardinal-bot",
	"version": "1.0.0",
	"main": "./dist/index.js",
	"author": "@sapphire",
	"license": "UNLICENSE",
	"type": "module",
	"imports": {
		"#config": "./dist/config.js",
		"#constants": "./dist/lib/util/constants.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/discord": "./dist/lib/discord/index.js",
		"#lib/setup": "./dist/lib/setup/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#utils/common": "./dist/lib/util/common/index.js",
		"#utils/functions": "./dist/lib/util/functions/index.js",
		"#root/*": "./dist/*.js"
	},
	"dependencies": {
		"@prisma/client": "^5.0.0",
		"@sapphire/decorators": "^6.0.1",
		"@sapphire/discord.js-utilities": "7.0.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.4.4",
		"@sapphire/plugin-editable-commands": "^3.0.2",
		"@sapphire/plugin-logger": "^3.0.4",
		"@sapphire/plugin-scheduled-tasks": "^7.1.0",
		"@sapphire/plugin-subcommands": "^4.0.1",
		"@sapphire/snowflake": "^3.5.1",
		"@sapphire/time-utilities": "^1.7.9",
		"@sapphire/utilities": "^3.12.0",
		"@skyra/env-utilities": "^1.2.1",
		"bullmq": "^4.2.1",
		"colorette": "^2.0.20",
		"discord.js": "^14.11.0",
		"enhanced-ms": "^2.3.0",
		"he": "^1.2.0",
		"lexure": "^0.17.0",
		"randomatic": "^3.1.1",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@sapphire/cli": "^1.6.1",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"@types/he": "^1",
		"@types/node": "^18.16.18",
		"@types/randomatic": "^3.1.3",
		"@types/ws": "^8.5.5",
		"prettier": "^2.8.8",
		"prisma": "^5.0.0",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.1.3"
	},
	"scripts": {
		"sapphire": "sapphire",
		"generate": "sapphire generate",
		"build": "tsc -b src",
		"watch": "tsc -w",
		"start": "node dist/index.js",
		"dev": "npm run build && npm run start",
		"watch:start": "tsc-watch --onSuccess \"npm run start\"",
		"format": "prettier --write \"src/\"",
		"prisma:push": "npx prisma db push",
		"prisma:generate": "npx prisma generate"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.6.1"
}
